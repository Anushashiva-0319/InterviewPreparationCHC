public class Pattern {

	public static void main(String[] args) {

		int a[][]= {{1,2},{1,3}};
		int b[][]= {{1,4},{1,3}};
		int sum;
		
		for(int i=0;i<2;i++) {
			for(int j=1;j<2;j++) {
				 sum =a[i][j]+b[i][j];
				 System.out.print(sum[i][j]+" ");    
			}    
			System.out.println();//new line    
			}    
		
	}
		
}
