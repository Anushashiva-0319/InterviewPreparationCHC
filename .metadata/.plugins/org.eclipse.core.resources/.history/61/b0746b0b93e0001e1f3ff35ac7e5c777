public class Pattern {

	public static void main(String[] args) {

		int a[]= {1,3,4,5,6,7};
		int start=0, end=a.length-1;
		
		
		int target=7;
		while(start<end) {
			int mid=start+(end-start)/2;
		if(target<a[mid]) {
			end=a[mid]-1;
		}
		else if(target>a[mid]) {
			start=a[mid]+1;
			System.out.println(start);
		}
	
		
	}
		
		String str="anusha kolli";
		str.trim();
		System.out.println(str);
	}
}
