import java.util.ArrayList;
import java.util.Arrays;
import java.util.Comparator;
import java.util.List;

public class CHECKINGINFo {

	public static void main(String[] args) {
		
		
		int num=12345;
		int count=0;
		while(num>0) {
			num=num/10;
			count++;
		}
		System.out.println(count);
		
		
		
}
}
