public class Pattern {

	public static void main(String[] args) {

		int a[]= {1,3,4,5,6,7};
		int start=0, end=a.length-1;
		int mid=start+(end-start)/2;
		
		int target=7;
		
		if(target<mid) {
			end=mid-1;
		}
		else if(target>mid) {
			start=mid+1;
			System.out.println(start);
		}
	
		
	}
	
}
