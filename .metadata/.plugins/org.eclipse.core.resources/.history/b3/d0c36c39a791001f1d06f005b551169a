import java.util.ArrayList;
import java.util.Arrays;
import java.util.Comparator;
import java.util.List;

public class CHECKINGINFo {

	public static void main(String[] args) {
		
		
		int num=12345678;
		int rem=0,rev=0;
		while(num>0) {
			rem=num/10;
			rev=rev*10+rem;
			num=num%10;
			
		}
		System.out.println(rev);
		
		
		
}
}
