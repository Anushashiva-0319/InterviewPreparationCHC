import java.util.Scanner;

public class prime1to100 {

	
	public static void main(String[] args) {
		
		int i,j,count=0;
		System.out.println("enter n value");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(i=1;i<=n;i++) {
			for(j=i;j>1;j--) {
				if(i%j==0) {
				count++;
			}
			if(count==2);
				
			System.out.println(j);
		}
		
	}
}
}
